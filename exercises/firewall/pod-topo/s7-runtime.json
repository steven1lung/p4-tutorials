{
  "target": "bmv2",
  "p4info": "build/basic.p4.p4info.txt",
  "bmv2_json": "build/basic.json",
  "table_entries": [
    {
      "table": "MyIngress.count_syn",
      "match": {
        "hdr.tcp.syn": 1
      },
      "action_name" : "MyIngress.update_syn",
      "action_params":{ }
    },
    {
      "table": "MyIngress.count_ack",
      "match": {
        "hdr.tcp.ack": 1
      },
      "action_name" : "MyIngress.update_ack",
      "action_params":{ }
    },
    {
      "table": "MyIngress.count_synack",
      "match": {
        "hdr.tcp.ack": 1,
        "hdr.tcp.syn": 1
      },
      "action_name" : "MyIngress.update_synack",
      "action_params":{ }
    },
    {
      "table": "MyIngress.dns_table",
      "match": {
        "hdr.dns.dqr": 0
      },
      "action_name" : "MyIngress.dns_question",
      "action_params":{ }
    },
    {
      "table": "MyIngress.dns_table",
      "match": {
        "hdr.dns.dqr": 1
      },
      "action_name" : "MyIngress.dns_answer",
      "action_params":{ }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "default_action": true,
      "action_name": "MyIngress.drop",
      "action_params": { }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.7.7", 32]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "08:00:00:00:07:77",
        "port": 1
      }
    }
  ]
}

